{
  "name": "samuelvi/spreadsheet-translator-core",
  "license": "MIT",
  "type": "library",
  "description": "Spreadsheet Translator - Core Module",
  "keywords": ["spreadsheet translations core module"],
  "homepage": "https://github.com/samuelvi/spreadsheet-translator-core",
  "authors": [
    {
      "name": "Samuel Vicent",
      "email": "samuelvicent@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Atico\\SpreadsheetTranslator\\Core\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Atico\\SpreadsheetTranslator\\Core\\Tests\\": "./tests"
    }
  },
  "require": {
    "php": ">=8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpdoc-parser": "^2.3",
    "rector/rector": "^2.1"
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage",
    "phpstan": "phpstan analyse src tests",
    "rector": "rector process --dry-run",
    "rector:fix": "rector process",
    "check": [
      "@test",
      "@phpstan"
    ]
  }
}
